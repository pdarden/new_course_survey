
<h2><%= @survey_questions["survey_title"] %></h2>

<% form_for @morning_survey,url: new_page_path do |f| %>
<ul>
<% @question_array.each_with_index do |question, index| %>
<li>
<%= f.label question %>
    <% f.hidden_field :question, question %>
  <% @option_array[index][0].each do |k,v| %>

    <span><%= f.label v %>
    <%= f.check_box v %></span>

    <%= f.submit "submit" %>
<%end%>
</li>
<%end%>
<%end%>
</ul>
<ul>
<% @question_array.each_with_index do |question, index| %>
<li>
<%= label_tag question %>
    <% hidden_field_tag question %>
  <% @option_array[index][0].each do |k,v| %>

    <span><%= label_tag v %>
    <%= check_box_tag v %></span>

<%end%>
</li>
<%end%>
</ul>

<%= link_to 'submit', pages_path(:question => "f", :answer => "this")%>

