
<h2><%= @survey_questions["survey_title"] %></h2>

<% form_for @page do |f| %>
<ul>
<% @question_array.each_with_index do |question, index| %>
<li>
<%= f.label question %>
    <% f.hidden_field :question, :value => question %>
  <% @option_array[index][0].each do |k,v| %>

    <span><%= f.label v %>
    <%= f.check_box :answer, :value => v %></span>

    <%= f.submit "submit" %>
<%end%>
</li>
<%end%>
<%end%>
</ul>

